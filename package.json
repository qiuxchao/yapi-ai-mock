{
	"name": "yapi-gpt-mock",
	"version": "1.0.0",
	"description": "利用GPT技术，将API文档生成为本地的Mock文件",
	"keywords": [
		"yapi",
		"codegen",
		"mockjs",
		"gpt"
	],
	"type": "module",
	"main": "./lib/index.cjs",
	"types": "./lib/index.d.ts",
	"files": [
		"lib"
	],
	"bin": {
		"ygm": "./lib/cli.cjs"
	},
	"scripts": {
		"build": "unbuild",
		"stub": "unbuild --stub",
		"dev": "chokidar \"src/**/*.ts\" -c \"pnpm build\"",
		"typecheck": "tsc --noEmit"
	},
	"exports": {
		".": {
			"types": "./lib/index.d.ts",
			"default": "./lib/index.cjs",
			"require": "./lib/index.cjs"
		}
	},
	"author": "",
	"license": "ISC",
	"peerDependencies": {
		"mockjs": "^1.1.0"
	},
	"dependencies": {
		"change-case": "^3.0.2",
		"consola": "^3.2.3",
		"crypto-js": "^4.1.1",
		"dayjs": "^1.11.9",
		"dotenv": "^16.3.1",
		"fs-extra": "^11.1.1",
		"node-fetch": "^2",
		"ora": "^3.4.0",
		"prettier": "^3.0.0",
		"ts-node": "^10.9.1",
		"typechat": "^0.0.10",
		"vtils": "^4.85.3",
		"yargs": "^16.2.0"
	},
	"devDependencies": {
		"@types/crypto-js": "^4.1.1",
		"@types/fs-extra": "^11.0.1",
		"@types/node": "^20.4.5",
		"@types/node-fetch": "^2.6.4",
		"@types/yargs": "^16.0.1",
		"chokidar-cli": "^3.0.0",
		"typescript": "^5.1.6",
		"unbuild": "^1.2.1"
	}
}

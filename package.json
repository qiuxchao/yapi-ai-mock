{
	"name": "yapi-mockjs-gpt",
	"version": "1.0.0",
	"description": "利用GPT技术，将API文档生成为本地的Mock文件",
	"keywords": [
		"yapi",
		"codegen",
		"gpt"
	],
	"type": "module",
	"main": "./lib/index.cjs",
	"types": "./lib/index.d.ts",
	"files": [
		"lib"
	],
	"scripts": {
		"build": "unbuild"
	},
	"exports": {
		".": {
			"import": "./lib/index.mjs",
			"require": "./lib/index.cjs"
		}
	},
	"author": "",
	"license": "ISC",
	"peerDependencies": {
		"mockjs": "^1.1.0"
	},
	"dependencies": {
		"consola": "^3.2.3",
		"node-fetch": "^3.3.1",
		"ora": "3.4.0"
	},
	"devDependencies": {
		"typescript": "^5.1.6",
		"unbuild": "^1.2.1"
	}
}

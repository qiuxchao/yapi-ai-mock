{
	"name": "yapi-gpt-mock",
	"version": "1.0.0",
	"description": "利用GPT技术，将API文档生成为本地的Mock文件",
	"keywords": [
		"yapi",
		"codegen",
		"mockjs",
		"chatgpt"
	],
	"type": "module",
	"main": "./lib/index.cjs",
	"types": "./lib/index.d.ts",
	"files": [
		"lib"
	],
	"bin": {
		"ygm": "./lib/cli.cjs"
	},
	"scripts": {
		"build": "unbuild",
		"stub": "unbuild --stub",
		"dev": "chokidar \"src/**/*.ts\" -c \"pnpm build\"",
		"typecheck": "tsc --noEmit"
	},
	"exports": {
		".": {
			"types": "./lib/index.d.ts",
			"default": "./lib/index.cjs",
			"require": "./lib/index.cjs"
		}
	},
	"author": "",
	"license": "ISC",
	"peerDependencies": {
		"@types/mockjs": "^1.0.7",
		"mockjs": "^1.1.0"
	},
	"dependencies": {
		"axios": "^1.4.0",
		"change-case": "^3.0.2",
		"chokidar": "^3.5.3",
		"co-body": "^6.1.0",
		"consola": "^3.2.3",
		"crypto-js": "^4.1.1",
		"dayjs": "^1.11.9",
		"debug": "^4.3.4",
		"dotenv": "^16.3.1",
		"fast-glob": "^3.2.12",
		"fs-extra": "^11.1.1",
		"ora": "^3.4.0",
		"path-to-regexp": "^6.2.1",
		"prettier": "^3.0.0",
		"ts-node": "^10.9.1",
		"typechat": "^0.0.10",
		"vite-plugin-mock-dev-server": "^1.3.0",
		"vtils": "^4.85.3",
		"yargs": "^16.2.0"
	},
	"devDependencies": {
		"@types/co-body": "^6.1.0",
		"@types/crypto-js": "^4.1.1",
		"@types/debug": "^4.1.7",
		"@types/fs-extra": "^11.0.1",
		"@types/node": "^20.4.5",
		"@types/path-to-regexp": "^1.7.0",
		"@types/yargs": "^16.0.1",
		"chokidar-cli": "^3.0.0",
		"typescript": "^5.1.6",
		"unbuild": "^1.2.1",
		"vite": "^4.4.9"
	}
}
